            <string>/// initialize stuff

// wich letter is currently selected
selected_button = "A";

// set global playername to be empty
global.playername = "";

// imposed character limit for names (starting 1)
character_limit = 8;
// uppercase letters
for (i = 65; i &lt; 91; i += 1) {

    array_index = i - 65;
    
    if (true) {
        xpos = ((array_index % 7) * 64) + 120;
        row_indicator = (array_index / 7);
        if (row_indicator &lt; 1 &amp;&amp; row_indicator &gt;= 0) { // 1st row
            ypos = 150;
        } else if (row_indicator &lt; 2 &amp;&amp; row_indicator &gt;= 1) { // 2nd row
            ypos = 150 + (28);
        } else if (row_indicator &lt; 3 &amp;&amp; row_indicator &gt;= 2) { // 3rd row
            ypos = 150 + (56);
        } else if (row_indicator &lt; 4 &amp;&amp; row_indicator &gt;= 3) { // 4th row
            ypos = 150 + (84);
        }
    }
    for (k = 0; k &lt; 3; k += 1) {
        if (k == 0) { // letter
            array_letters[array_index,k] = chr(i);
        } else if (k == 1) { // x pos
            array_letters[array_index,k] = xpos;
        } else if (k == 2) { // y pos
            array_letters[array_index,k] = ypos;
        }
    }
}

// lowercase letters
for (v = 97; v &lt; 123; v += 1) {

    array_index_lower = v - 71;
    
    if (true) {
        xpos = (((array_index_lower + 2) % 7) * 64) + 120;
        row_indicator = ((array_index_lower + 2) / 7);
        if (row_indicator &lt; 5 &amp;&amp; row_indicator &gt;= 4) { // 5th row
            ypos = 272;
        } else if (row_indicator &lt; 6 &amp;&amp; row_indicator &gt;= 5) { // 6th row
            ypos = 272 + (28);
        } else if (row_indicator &lt; 7 &amp;&amp; row_indicator &gt;= 6) { // 7th row
            ypos = 272 + (56);
        } else if (row_indicator &lt; 8 &amp;&amp; row_indicator &gt;= 7) { // 8th row
            ypos = 272 + (84);
        }
    }
    for (h = 0; h &lt; 3; h += 1) {
        if (h == 0) { // letter
            array_letters[array_index_lower,h] = chr(v);
        } else if (h == 1) { // x pos
            array_letters[array_index_lower,h] = xpos;
        } else if (h == 2) { // y pos
            array_letters[array_index_lower,h] = ypos;
        }
    }
}
