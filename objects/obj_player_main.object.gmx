            <string>/// movement and room transition
//gets input for movement
scr_getinput();

//sets phixed rotation in physics room
phy_fixed_rotation = true;

//checks if player is in dialog (cannot move while dialog plays)
if (global.dialog == true) {
    state = scr_nomove_state;
} else if (encounter_full = true || encounter_transition = true || encounter_start == true){
    state = scr_nomove_state;
}

//teleports player to the entryway from the door they took
if (room != lastroom) {
    phy_position_x = newx;
    phy_position_y = newy;
    lastroom = room;
}

// enter encounter when F is pressed ( for testing fight mech stuff )
if (keyboard_check(ord("F")) == true and canencounter == true) {
    scr_encounter(1);
}

if (encounter_transition == true) {
    image_alpha = 0;
} else {
    image_alpha = 1;
}

//updates current player position every frame
playerpos[0] = phy_position_x;
playerpos[1] = phy_position_y;

//moves or whatever based on the state of the player (set above &amp; in create)
script_execute(state);
</string>
