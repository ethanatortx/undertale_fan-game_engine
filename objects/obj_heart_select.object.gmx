            <string>/// initialize monster stats

if (instance_exists(obj_player_main)) {
    obj_player_main.image_alpha = 0;
}

// *** TESTING PURPOSES ONLY ***
//global.current_encounter[0] = mob_testmonster;

// set up 2d stat array and hp pool array
scr_initialize_fight_variables(global.current_encounter);

// start whith flashing the heart
heart_flashing = 1;

inbox = 0; // whether or not the heart is in the box

backwards = false;
animation_pos = undefined;
anim_speed = undefined;
roomwidth = obj_player_main.last_room_width;
roomheight = obj_player_main.last_room_height;
x_flash_start = undefined;
y_flash_start = undefined;
if (roomwidth - obj_player_main.x &lt; 160) {
    x_flash_start = obj_player_main.x;
} else if (obj_player_main.x - 160 &lt; 0) {
    x_flash_start = 320 - (roomwidth - floor(obj_player_main.x));
} else {
    x_flash_start = 320;
}
if (roomheight - obj_player_main.y &lt; 120) {
    y_flash_start = obj_player_main.y;
} else if (obj_player_main.y - 120 &lt; 0) {
    y_flash_start = 240 - (roomheight - floor(obj_player_main.y));
} else {
    y_flash_start = 240;
}

new_xpos = x_flash_start;
new_ypos = y_flash_start;

current_x = x_flash_start;
current_y = y_flash_start;

position[0] = x_flash_start;
position[1] = y_flash_start;

global.buttonpos = 4;

page_items = 0;
selected_item = 0;
item_index_for_use = 0;

heart_tick_count = 0;

for (i = 0; i &lt; array_height_2d(global.monster_stats); i++) {
    if (global.monster_stats[i,1] &gt; 0) {
        current_sprites = script_execute(asset_get_index("mob_" + global.monster_stats[i,0] + "_txtr"));
        all_monster_sprites[i] = current_sprites;
    }
}
