          <argument>
            <kind>1</kind>
            <string>/// shake the name and move between options

// move x
if (starting_position[0] &gt; final_position[0]) {
    starting_position[0] -= .5;
}
// move y
if (starting_position[1] &lt; final_position[1]) {
    starting_position[1] += .75;
}
// increase x scale
if (starting_position[0] &gt; final_position[0]) {
    x_scale += .0125;
}
// increase y scale
if (starting_position[1] &lt; final_position[1]) {
    y_scale += .0125;
}

if (j == 0) {
    // randomize rotation
    name_rotation = random_range(-1,1);
    // randomize shake value
    name_shake = random_range(-1,1);
    j++;
} else if (j == 1) {
    j--;
}

if (start_transition = true &amp;&amp; transition_alpha &lt; 1.3) {
    transition_alpha += .004;
} else if (transition_alpha &gt;= 1.3) {
    room_goto_next();
    instance_destroy();
}

if ((keyboard_check_pressed(vk_right) || keyboard_check_pressed(vk_left)) &amp;&amp; !global.pre_chosen) {
    if (global.buttonpos = 0) {
        global.buttonpos = 1;
    } else if (global.buttonpos = 1) {
        global.buttonpos = 0;
    }
}
</string>
