        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Do talking and show up animation

if (appear_anim) {
    anim_frame -= 0.2;
    animating = true;
    current_face = spr_flowey_appear_anim;
    if (anim_frame &lt;= 0) {
        animating = false;
        appear_anim = false;
        anim_frame = 0;
    }
} else if (disappear_anim) {
    anim_frame += 0.5;
    animating = true;
    current_face = spr_flowey_appear_anim;
    if (anim_frame &gt;= 5) {
        anim_frame = 5;
        disappear_anim = false;
        animating = false;
    }
} else if (anim_frame &lt;= 0) {
    animating = false;
    anim_frame = 0;
} else if (anim_frame &gt;= 5) {
    anim_frame = 5;
    animating = false;
}

if (!animating) {
    if (keyboard_check_pressed(vk_enter) &amp;&amp; characters == dialog_line_length) {
        start_drawing_text = true;
        dialog_line_length = string_length(draw_dialog_lines[i]);
        characters = 0;
        i += 1;
        current_drawn_text = ""
    } else if (start_drawing_text &amp;&amp; characters &lt; dialog_line_length) {
        characters += talking_speed;
        current_drawn_text = string_copy(draw_dialog_lines[i,0],1,characters);
    }
}
</string>
